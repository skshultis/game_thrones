<% @house.errors.full_messages.each do |message| %>
  <p>
    <%= message %>
  </p>
  <% end %>
<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
  <%= f.submit %>
<% end %>
