<h1><%= @character.name %></h1>
<div class="char">
    <img src="<%= @character.photo_url %>">
</div>
<p><%= link_to character_path(@character.house), class:'linkto' %></p>
<p><%= link_to 'delete character', character_path(@character), method: :delete, data:{confirm: "Are you sure you want to delete this character?"}, class:'linkto' %></p>
<p><%= link_to "create new character", new_character_path, class:'linkto' %></p>
<p><%= link_to 'back', characters_path, class:'linkto' %></p>
