<%= form_for @character do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
  <%= f.label :house_id %>
  <%= f.select :house_id,  options_for_select(@houses.map {|new_house| [new_house.name, new_house.id]}) %>
  <%= f.submit %>
<% end %>
